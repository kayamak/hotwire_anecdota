<turbo-frame id="user-profile">
  <div>
    <p>
      <strong>User:</strong>
      <%= @user_profile.user_id %>
    </p>

    <p>
      <strong>Name:</strong>
      <%= @user_profile.name %>
    </p>

    <p>
      <strong>Name jp:</strong>
      <%= @user_profile.name_jp %>
    </p>

    <p>
      <strong>Age:</strong>
      <%= @user_profile.age %>
    </p>
  </div>
</turbo-frame>
